{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [ false ],
        "interface-name": [],
        "variable-name": ["allow-snake-case"],
        "quotemark": [ true, "single" ],
        "object-curly-spacing": [2, "always", {
            "objectsInObjects": true,
            "arraysInObjects": false
        }],
        "max-line-length": [false],
        "no-debugger": false,
        "no-console": [false]
    },
    "rulesDirectory": []
}
