{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [ false ],
        "quotemark": [ true, "single" ],
        "interface-name": false,
        "object-curly-spacing": [2, "always", {
            "objectsInObjects": true,
            "arraysInObjects": false
        }]
    },
    "rulesDirectory": []
}